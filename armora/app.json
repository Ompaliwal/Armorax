{
  "expo": {
    "name": "Armora-X",
    "slug": "Armora-X",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/AppIcons/appstore.png",
    "scheme": "Armora-X",
    "extra": {
      "apiUrl": "http://192.168.1.5:4000"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/AppIcons/playstore.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.CAMERA"
      ],
      "package": "com.ompaliwal.ArmoraX"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/AppIcons/appstore.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
